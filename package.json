{
  "name": "cmutils",
  "version": "1.0.0",
  "description": "A library for common utility functions",
  "main": "index.js",
  "scripts": {
    "start": "npm run tsc && node build/index.js",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jsdocs": "jsdoc build/*.js"
  },
  "keywords": [
    "common utils",
    "utils"
  ],
  "author": "dtsuper3",
  "license": "ISC",
  "dependencies": {
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jsdoc": "^3.6.6",
    "prettier": "^2.1.2"
  }
}